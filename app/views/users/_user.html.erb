<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Photo:</strong>
    <%= user.photo %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= user.bio %>
  </p>

  <p>
    <strong>Updatedat:</strong>
    <%= user.updatedAt %>
  </p>

  <p>
    <strong>Createdat:</strong>
    <%= user.createdAt %>
  </p>

  <p>
    <strong>Postscounter:</strong>
    <%= user.postsCounter %>
  </p>

</div>
